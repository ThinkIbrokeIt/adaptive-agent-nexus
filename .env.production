# Production Environment Configuration
NODE_ENV=production

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# LLM API Keys
VITE_OPENAI_API_KEY=your-openai-key-here
VITE_ANTHROPIC_API_KEY=your-anthropic-key-here
VITE_OLLAMA_BASE_URL=https://ollama.your-domain.com

# Storage Configuration
VITE_DUCKDB_PATH=/data/duckdb
VITE_CHROMADB_URL=https://chromadb.your-domain.com
VITE_MINIO_ENDPOINT=minio.your-domain.com
VITE_MINIO_ACCESS_KEY=your-minio-access-key
VITE_MINIO_SECRET_KEY=your-minio-secret-key

# Application Configuration
VITE_APP_TITLE="Adaptive Agent Nexus"
VITE_APP_VERSION="0.6.0-alpha"
VITE_API_BASE_URL=https://api.adaptive-agent-nexus.com

# Feature Flags
VITE_ENABLE_VOICE=true
VITE_ENABLE_FEEDBACK_LOOP=true
VITE_ENABLE_REAL_WORKFLOWS=true

# Analytics & Monitoring (optional)
VITE_SENTRY_DSN=your-sentry-dsn-here
VITE_GA_TRACKING_ID=your-ga-id-here